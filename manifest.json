{
    "manifest_version": 3,
    "name": "Custom new tab",
    "short_name": "custom-new-tab",
    "version": "1.0.0",
    "description": "Custom new tab page with minimal implementation and permissions.",
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "options_ui": {
        "page": "options.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "custom-new-tab@csutorasa.github.com",
            "strict_min_version": "74.0"
        }
    },
    "minimum_chrome_version": "88"
}